{
  "headers": [
    {
      "source": "/api/imagem.png",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/png"
        },
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/imagem.png/:any*",
      "destination": "/api/imagem.png"
    },
    {
      "source": "/imagem.png",
      "destination": "/api/imagem.png"
    },
    {
      "source": "/download/:any*",
      "destination": "/api/imagem.png"
    },
    {
      "source": "/assets/:any*",
      "destination": "/api/imagem.png"
    }
  ]
}
